	"jQuery AJAX form data" : {
		"prefix": "rr-ajax-formdata",
		"body":[
			"let form = new FormData(e.target);",
			"form.append(\"x\", 8);",
			"form.append(\"z\", \"ucn\");",
	
			"$.ajax({",
				"type: \"POST\",",
				"url: \"/profiling/won.php\",",
				"data: form,",
				"processData: false,",
				"contentType: false,",
				"success: function (data) {",
					"let d = JSON.parse(data);",
					"if (d.status == 0) {",
						"Toast.fire({",
							"icon: \"info\",",
							"title: \"Changes saved!.\",",
						"});",
					"} else if (d.status == 1) {",
						"won.error(d.data);",
					"}",
				"},",
				"error: function (ex) {",
					"console.log(ex);",
				"},",
			"});",
			],
		"description": "For AJAX JQuery form data"
	},
